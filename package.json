{
  "name": "rkta-starter",
  "version": "1.0.1",
  "description": "Create universal react apollo apps",
  "main": "jest.config.js",
  "engines": {
    "node": ">=12.16.0"
  },
  "sideEffects": false,
  "scripts": {
    "preanalyze": "npm run build:client",
    "analyze": "webpack-bundle-analyzer build/stats.json public",
    "prebuild": "npm run checkall",
    "build:client": "NODE_ENV=production webpack --json > build/stats.json",
    "build:server": "babel -x .ts,.tsx -D src --out-dir build --source-maps inline",
    "now-build": "npm run clean; CLIENT__LOGUX_URL=wss://$VERCEL_URL NODE_ENV=production webpack --hide-modules --json > build/stats.json",
    "build": "run-p -l build:**",
    "checkall": "npm run clean && npm run typecheck && npm run lint && npm run test",
    "clean": "rimraf {build,public/{b-,sw.,workbox}*js*} && mkdirp build && mkdirp public",
    "predeploy": "npm run checkall",
    "deploy": "now --prod",
    "dev": "NODE_ENV=development nodemon -w src/koa -w src/logux -w .env -e ts,tsx -x 'babel-node -x .ts,.tsx scripts/start-dev-server'",
    "lint": "eslint 'src' --ext .ts,.tsx",
    "start": "NODE_ENV=production node --enable-source-maps build/koa",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc",
    "typecheck:watch": "npm run typecheck -- --watch",
    "updatedeps": "ncu -u && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r-k-t-a/starter.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/r-k-t-a/starter/issues"
  },
  "homepage": "https://github.com/r-k-t-a/starter#readme",
  "dependencies": {
    "@emotion/cache": "^10.0.29",
    "@emotion/core": "^10.0.35",
    "@logux/core": "^0.6.2",
    "@logux/redux": "^0.7.1",
    "@logux/server": "^0.9.6",
    "@rkta/context": "^0.0.163",
    "@rkta/entypo": "^0.0.163",
    "@rkta/ui": "^0.0.163",
    "@types/react-router-dom": "^5.1.5",
    "create-emotion-server": "^10.0.27",
