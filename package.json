{
  "name": "rkta-starter",
  "version": "1.0.0",
  "description": "Create universal react apollo apps",
  "main": "jest.config.js",
  "scripts": {
    "preanalyze": "npm run build:client",
    "analyze": "webpack-bundle-analyzer build/stats.json public",
    "prebuild_bak": "npm run clean && npm run typecheck && npm run lint && npm run test",
    "prebuild": "npm run clean && npm run typecheck && npm run lint",
    "build:client": "NODE_ENV=production webpack --json > build/stats.json",
    "build:backend": "tsc -p tsconfig.backend.json --outDir build",
    "build": "run-p -l build:**",
    "clean": "rimraf {.dev,build,public/{b-,sw.,workbox}*js*} && mkdirp build && mkdirp public",
    "dev": "cross-env NODE_ENV=development nodemon -e ts,tsx -w ./src/server -w webpack.config.js -x 'node scripts/start-dev-server'",
    "lint": "eslint 'src' --ext .ts,.tsx",
    "start": "NODE_ENV=production node scripts/start-prod-server",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc",
    "typecheck:watch": "npm run typecheck -- --watch"
  },
