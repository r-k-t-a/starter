{
  "name": "@rkta-starter/client",
  "version": "1.0.0",
  "scripts": {
    "preanalize": "npm run build:client",
    "analyze": "webpack-bundle-analyzer build/stats.json ../public",
    "prebuild": "npm run clean && npm run typecheck && npm run lint && npm run test",
    "build": "NODE_ENV=production webpack --json > build/stats.json",
    "clean": "rimraf {.dev,build,../public/b*.js} && mkdirp build && mkdirp ../public",
    "dev": "cross-env NODE_ENV=development nodemon -e ts,tsx -w ./src/server -w webpack.config.js -x 'node scripts/start-dev-server'",
    "lint": "eslint 'src' --ext .ts,.tsx",
    "start": "NODE_ENV=production node scripts/start-prod-server",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc -p . --emitDeclarationOnly",
    "typecheck:watch": "npm run typecheck -- --watch"
  },
